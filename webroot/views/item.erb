<!-- Content Header (Page header) -->
<section class="content-header">
<h1>Item
<%- @group_breadcrumb.each do |bc| -%>
  <span class="icon">
  <i class="icon fa fa-angle-double-right" aria-hidden="true"></i>
  </span>
  <a href="<%= bc[:path] %>"><%= bc[:name] %></a>
<%- end -%>
  <span class="icon">
  <i class="icon fa fa-angle-double-right" aria-hidden="true"></i>
  </span>
  #<%= @item.id %>
</h1>
</section>

<!-- Main content -->
<section class="content">

<div class="box">
  <div class="box-header with-border">
    <h3 class="box-title">#<%= @item.id %> : <%= @item.name %></h3>
  </div>
<div class="box-body">
<table class="table table-bordered">
  <tbody>
  <%- %w(message_id name group level type address message misc received_at expire_at).each do |col| -%>
  <tr <% if col == 'level' %>class="<%= @item.style[:alert] %>"<% end %>>
    <th><%= col.extend(CamelSnake).to_pascal %></th>
    <td><%= @item.prop_for_web(col) %></td>
  </tr>
  <%- end -%>
  </tbody>
</table>
</div>
<!-- /.box-body -->
</div>
<!-- /.box -->

</section>
<!-- /.content -->
